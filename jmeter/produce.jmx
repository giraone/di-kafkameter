          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import com.github.f4b6a3.tsid.TsidCreator;

import java.time.Instant;
import java.time.ZoneId;
import java.time.format.DateTimeFormatter;

String messageKey = TsidCreator.getTsid256().toString();
vars.put(&quot;messageKey&quot;, messageKey);

String messageContent = &quot;Content &quot; + messageKey;
vars.put(&quot;messageContent&quot;, messageContent);

String timestampCreated = DateTimeFormatter.ISO_INSTANT.withZone(ZoneId.systemDefault()).format(Instant.now());
vars.put(&quot;timestampCreated&quot;, timestampCreated);</stringProp>
